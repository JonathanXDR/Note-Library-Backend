generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Book {
  id          Int    @id @default(autoincrement())
  title       String @db.VarChar(100)
  releaseYear Int    @db.Year()
  author      String @db.VarChar(30)
}
